{
  "name": "blank-project-elysia",
  "version": "1.0.0",
  "description": "A modern, high-performance API server built with Elysia and Bun runtime",
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist --target bun",
    "lint": "bun run --bun eslint src/**/*.ts",
    "typecheck": "bun --bun tsc --noEmit"
  },
  "keywords": [
    "elysia",
    "bun",
    "api",
    "typescript",
    "jwt",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@elysiajs/jwt": "^1.3.2",
    "@elysiajs/swagger": "^1.3.1",
    "elysia": "latest",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/pg": "^8.15.5",
    "bun-types": "latest"
  }
}